    "import json\n",
    "from kafka import KafkaConsumer\n",
    "\n",
    "consumer = KafkaConsumer(bootstrap_servers='kafka:9092')\n",
    "consumer.subscribe(['default_topic', 'consumer_topic'])\n",
    "\n",
    "while 1:\n",
    "  for msg in consumer:\n",
    "    print (json.loads(msg.value.decode('utf-8')))"
